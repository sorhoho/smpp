# General configuration
user nginx;
worker_processes 2;

error_log /var/log/nginx/error.log warn;
pid /var/run/nginx.pid;

events {
  worker_connections 1024;
}

# Load balancing configuration starts here
#
#
#

stream {
    upstream backend {
        server localnode:2775;
        server localnode:2776;
        server localnode:2777;
    }

    server {
        listen 80;
        proxy_pass backend;
    }
}
